{
  "name": "csv-viewer",
  "displayName": "CSV Viewer",
  "description": "A CSV viewer extension for VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:csv",
    "onLanguage:dynamic-csv"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "csvViewer.openPreview",
        "title": "Open CSV Preview"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.x.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.x.x",
    "@typescript-eslint/parser": "^4.x.x",
    "eslint": "^7.x.x",
    "typescript": "^4.x.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/csv-viewer.git"
  },
  "publisher": "YourPublisherName",
  "dependencies": {
    "csv-parse": "^5.5.6"
  }
}
